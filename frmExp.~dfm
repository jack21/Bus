  object ExpQuery: TADOQuery
    CacheSize = 1000
    Connection = DM.DBConn
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    Parameters = <
      item
        Name = 'myYear'
        Attributes = [paNullable]
        DataType = ftWideString
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = '2004'
      end
      item
        Name = 'myMonth'
        Attributes = [paNullable]
        DataType = ftWideString
        NumericScale = 255
        Precision = 255
        Size = 510
        Value = '1'
      end>
    SQL.Strings = (
      'SELECT *'
      'FROM Exp'
      
        'WHERE Years=:myYear AND Months=:myMonth and BusNumber in (Select' +
        ' BusNumber From Bus)'
      'Order By BusNumber'
      ' '
      ' ')
